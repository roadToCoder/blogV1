---
import BaseLayoutPageVide from '../../layouts/BaseLayoutPageVide.astro';
const tags = ["astro", "successes", "community", "setbacks", "learning in public"];
const pageTitle = "Tag Index";
---
<BaseLayoutPageVide pageTitle={pageTitle}>
    <div>
        {tags.map((tag) => (
            <p><a href={`/tags/${tag}`}>{tag}</a></p>
            ))}
    </div>
</BaseLayoutPageVide>