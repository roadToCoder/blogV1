<div aria-label="Site Header" class="fixed z-40 w-full bg-white border-b border-sky-600 mb-1 shadow-inner ">
    <div class="flex items-center justify-between mx-auto max-w-screen-xl px-4 sm:px-6 lg:px-8">
      <div class="w-60 max-w-full px-4 ">
        <a href="javascript:void(0)" class="block w-full py-3">
          <img src="/img/logo.png" alt="logo" class="w-full"/>
        </a>
      </div> 
      <div class="py-1 md:flex md:items-center md:gap-12 ">
        <nav aria-label="navbar" class="hidden md:block pl-4 ">
          <ul class="flex gap-6 text-sm ">
            <li>
              <a class="px-1 font-medium text-lg text-sky-800 border-b-2 border-transparent hover:text-indigo-700 transition-colors duration-300 transform dark:hover:text-gray-200 hover:border-blue-500 inline-block align-middle mt-2" href="/">Accueil</a>
            </li>
            <li>
              <a class="px-1 font-medium text-lg text-sky-800 border-b-2 border-transparent hover:text-indigo-700 transition-colors duration-300 transform dark:hover:text-gray-200 hover:border-blue-500 inline-block align-middle mt-2" href="/skills/">Skills</a>
            </li>
            <li>
              <a class="px-1 font-medium text-lg text-sky-800 border-b-2 border-transparent hover:text-indigo-700 transition-colors duration-300 transform dark:hover:text-gray-200 hover:border-blue-500 inline-block align-middle mt-2" href="/about/">A propos</a>
            </li>
            <li>
              <a class="px-1 font-medium text-lg text-sky-800 border-b-2 border-transparent hover:text-indigo-700 transition-colors duration-300 transform dark:hover:text-gray-200 hover:border-blue-500 inline-block align-middle mt-2" href="/contact/">Contact</a>
            </li> 
          </ul>
        </nav>   
        <div class="block md:hidden">
          <button class="rounded bg-gray-100 p-2 text-gray-600 transition hover:text-gray-600/75 inline-block align-middle mt-1">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              class="h-5 w-5"
              fill="none"
              viewBox="0 0 24 24"
              stroke="currentColor"
              stroke-width="2"
            >
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                d="M4 6h16M4 12h16M4 18h16"
              />
            </svg>
          </button>
        </div>
        
      </div>
    </div>
</div> 